!! URxvt Appearance
!URxvt.font: xft:inconsolata:size=8:antialias=true

URxvt.font: xft:inconsolata:size=8:antialias=true:hinting=slight

!URxvt.boldFont: -*-rissole-*
!URxvt.boldFont: -*-rissole-*
!URxvt.italicFont: -*-rissole-*
!URxvt.boldItalicfont: -*-rissole-*
URxvt*borderless: 1
URxvt.letterSpace: 0 
URxvt.lineSpace: 0
URxvt.geometry: 92x24
URxvt.internalBorder: 18
URxvt.cursorBlink: true
URxvt.cursorUnderline: false
URxvt.saveline: 2048
URxvt.scrollBar: false
URxvt.scrollBar_right: false
URxvt.urgentOnBell: true
URxvt.depth: 24

URxvt*iso14755: False
URxvt*iso14755_52: False



!! Common Keybinds for Navigations
URxvt.keysym.Shift-Up: \033[1;2A
URxvt.keysym.Shift-Down: \033[1;2B 
URxvt.keysym.Control-Up: \033[1;5A
URxvt.keysym.Control-Down: \033[1;5B
URxvt.keysym.Control-Right: \033[1;5C
URxvt.keysym.Control-Left: \033[1;5D
URxvt.keysym.Control-End: \033[1;5F
URxvt.keysym.Control-Home: \033[1;5H


!! Copy Paste & Other Extensions
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard

!! Terminal URL
URxvt.urlLauncher: google-chrome
URxvt.underlineURLs: true
!URxvt.urlButton: 1



!! xterm Settings
xterm*faceName:             inconsolata 
xterm*faceSize:             8
xterm.vt100.saveLines:      1000
xterm*dynamiccolors:        true
xterm*utf8:                 2
xterm*geometry:             WINDOWGEOMETRY
xterm*visualBell:           off
xterm*highlightSelection:   true


!! Copy-Paste
xterm*selectToClipboard: true
xterm*VT100*translations: #override \n\
    Ctrl Shift <Key>V:  insert-selection(CLIPBOARD) \n\
    Ctrl Shift <Key>C:        select-end(CLIPBOARD)

!! st Simple Terminal Settigs
st.alpha: 1.0
st.font: Monospace:size=8:antialias=true:autohint=true
st.borderpx: 15
