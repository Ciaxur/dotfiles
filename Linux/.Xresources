!! URxvt Transparency
!URxvt.transparent: true
!URxvt.shading: 10



!! URxvt Appearance
URxvt.font: xft:inconsolata:size=8:antialias=true
URxvt*borderless: 1
!URxvt.boldFont: -*-rissole-*
!URxvt.boldFont: -*-rissole-*
!URxvt.italicFont: -*-rissole-*
!URxvt.boldItalicfont: -*-rissole-*
URxvt.letterSpace: 0
URxvt.lineSpace: 0
URxvt.geometry: 92x24
URxvt.internalBorder: 18
URxvt.cursorBlink: true
URxvt.cursorUnderline: false
URxvt.saveline: 2048
URxvt.scrollBar: false
URxvt.scrollBar_right: false
URxvt.urgentOnBell: true
URxvt.depth: 24
URxvt.iso14755: false



!! Common Keybinds for Navigations
URxvt.keysym.Shift-Up: \033[1;2A
URxvt.keysym.Shift-Down: \033[1;2B 
URxvt.keysym.Control-Up: \033[1;5A
URxvt.keysym.Control-Down: \033[1;5B
URxvt.keysym.Control-Right: \033[1;5C
URxvt.keysym.Control-Left: \033[1;5D
URxvt.keysym.Control-End: \033[1;5F
URxvt.keysym.Control-Home: \033[1;5H


!! Copy Paste & Other Extensions
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard

!! Terminal URL
URxvt.urlLauncher: google-chrome
URxvt.underlineURLs: true
!URxvt.urlButton: 1

